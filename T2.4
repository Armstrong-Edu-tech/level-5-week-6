import React from "react";


function UserCard({ name, age = 18 }) {

  if (typeof name !== "string") return null;
  if (typeof age !== "number") return null;

  return (
    <div className="user-card">
      <h2>Name: {name}</h2>
      <p>Age: {age}</p>
    </div>
  );
}

function App() {
  return (
    <div>
      <UserCard name="Alice" age={30} />
      <UserCard name="Bob" />

      <UserCard name={123} age="twenty" />
    </div>
  );
}

export default App;
